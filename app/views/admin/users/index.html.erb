<h1>All Users</h1>

<%= link_to 'New user', new_user_path %>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>email</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>